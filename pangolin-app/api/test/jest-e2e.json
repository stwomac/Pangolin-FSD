{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "modulePaths": ["."],
  "rootDir": "..",
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  },
  "setupFilesAfterEnv": ["jest-extended/all"]
}
